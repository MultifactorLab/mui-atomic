{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mui-atomic": {
      "projectType": "library",
      "root": "projects/mui-atomic",
      "sourceRoot": "projects/mui-atomic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/mui-atomic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/mui-atomic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/mui-atomic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/mui-atomic/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/mui-atomic/.storybook",
            "browserTarget": "mui-atomic:build",
            "compodoc": false,
            "port": 6006,
            "styles": [
              "projects/mui-atomic/src/lib/styles/theme/palette.scss",
              "projects/mui-atomic/src/lib/styles/theme/typography.scss",
              "projects/mui-atomic/src/lib/styles/storybook/storybook-dark.scss"
            ]
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/mui-atomic/.storybook",
            "browserTarget": "mui-atomic:build",
            "compodoc": false,
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
